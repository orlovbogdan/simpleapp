if (<%= !!params[:endless] %>){
    $('#content').append('<%=j render(template: 'users/index.html') %>')
} else {
    $('#content').html('<%=j render(template: 'users/index.html') %>')
}
$('#search_form input').trigger('focus');
document.title = "<%= escape_javascript("#{params[:search].to_s.titleize} Users by #{(params[:sort] || 'name').titleize} - Page #{params[:page] || 1}") %>";